generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Room {
  id        String  @id @map("_id")
  title     String
  isPrivate Boolean
  password  String?
  Users      User[]
  hash String
}

model User {
  id     String @id @map("_id")
  name   String? @unique()
  room   Room   @relation(fields: [roomId], references: [id])
  roomId String
  socketId String
}